'''
S = [10,20,30,40,50]
S.insert(2,25)
print(S)
# Output: [10, 20, 25, 30, 40, 50]
S.remove(40)    
print(S)
# Output: [10, 20, 25, 30, 50]
S.append(60)
print(S)
# Output: [10, 20, 25, 30, 50, 60]
S.pop()
print(S)
# Output: [10, 20, 25, 30, 50]
S.reverse()
print(S)
# Output: [50, 30, 25, 20, 10]
S.sort()
print(S)
# Output: [10, 20, 25, 30, 50]
